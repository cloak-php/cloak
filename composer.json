{
    "name": "cloak/cloak",
    "keywords": [ "code coverage", "simple" ],
    "description": "cloak is a library that takes a code coverage.",
    "version": "1.3.0",
    "require": {
        "php": ">=5.4.0",
        "phpcollection/phpcollection": "0.4.*",
        "kevinlebrun/colors.php": "0.3.*",
        "zendframework/zend-eventmanager": "2.3.*"
    },
    "autoload": {
        "psr-4": { "cloak\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "cloak\\spec\\driver\\": "spec/fixtures/"
        }
    },
    "require-dev": {
        "danielstjules/pho": "1.0.*",
        "jaz303/phake": "0.6.*",
        "holyshared/coverallskit": "1.2.2",
        "mockery/mockery": "0.9.*"
    },
